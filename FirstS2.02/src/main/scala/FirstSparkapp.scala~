object FirstSparkapp {
  def main(args: Array[String]) {
    val conf = new SparkConf().setAppName("FirstSparkproject").setMaster("local")
    val sc = new SparkContext(conf)
        println("*******************")  
        println("First spark project")
        println("*******************")  
  }
}
